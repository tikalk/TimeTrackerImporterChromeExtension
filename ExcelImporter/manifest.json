{
  "name" : "Tikal Excel Importer (By Oren Farhi))",
  "version": "2.0",
  "manifest_version" : 2,
  "description" : "Copy & Paste an excel sheet or a table format of an entire month to Tikal's time tracker. By Oren Farhi 2012",
  "background" : {
    "page": "background.html"
    },
  "page_action" :
  {
    "default_title" : "Tikal Excel Importer"
  },
  "content_scripts" : [
    {
      "matches" : [
        "https://planet.tikalk.com/timetracker/mytime.php"
      ],
      "js" : ["jquery.min.js", "contentscript.js"],
      "run_at" : "document_idle",
      "all_frames" : true
    }
  ],
  "sandbox": {
      "content_security_policy": [
        "sandbox allow-scripts; script-src https://ajax.googleapis.com"
      ]
    },
  "web_accessible_resources": [
    "tikalTimePosting.js"
  ],
  "update_url": "http://tikalk.github.com/TimeTrackerImporterChromeExtension/ExcelImporter-updates.xml"
}
